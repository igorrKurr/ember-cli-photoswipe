<h2 id="title">Photoswipe Examples</h2>

<!-- example 1 -->

{{#photo-swipe tagName='button' options=psOpts content=items withoutThumbs=true}}
  Open Gallery
{{/photo-swipe}}

<br>

<!-- example 2 -->
<h3>Example when passing a block</h3>

{{#photo-swipe options=psTwoOpts as |gallery|}}
  {{#each model as |img index|}}
    {{photoswipe-item class='thumb' src=img.href title=img.alt gallery=gallery number=index action='toggleImage'}}
  {{/each}}
{{/photo-swipe}}
